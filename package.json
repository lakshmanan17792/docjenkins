{
  "name": "ita",
  "description": "Intelligent Talent Acquisition",
  "author": "Ideas2IT",
  "version": "2.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ideas2IT/ita-ui.git"
  },
  "homepage": "https://github.com/Ideas2IT/ita-ui.git",
  "keywords": [
    "javaji",
    "talentsteps"
  ],
  "main": "bin/server.js",
  "scripts": {
    "precommit": "lint-staged",
    "precommit-prettier": "eslint --print-config .eslintrc.js | prettier-eslint --write",
    "start": "concurrently --kill-others \"npm run start-prod\"",
    "start-prod": "better-npm-run start-prod",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "build": "better-npm-run build",
    "build-dlls": "webpack --colors --display-error-details --config webpack/vendor.config.js",
    "postinstall": "concurrently \"npm run build\" \"npm run build-dlls\"",
    "prettier": "cd ita-ui && npm run precommit-prettier -- \"./+(src)/**/*.js\"",
    "lint": "eslint -c .eslintrc src",
    "test": "karma start",
    "docker-build": ". ./docker/build.sh",
    "docker-dev": ". ./docker/run.dev.sh",
    "docker-prod": ". ./docker/run.prod.sh",
    "lighthouse-report": "LIGHTHOUSE_CHROMIUM_PATH=$(which google-chrome) lighthouse http://localhost:8080"
  },
  "lint-staged": {
    "+(src|api)/**/*.js": [
      "cd ita-ui && npm run precommit-prettier",
      "eslint -c .eslintrc",
      "git add"
    ]
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3000
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 4000,
        "APIPORT": 3000,
        "WEBPACK_DLLS": 1
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 4000,
        "APIPORT": 3000,
        "WEBPACK_DLLS": 1
      }
    },
    "build": {
      "command": "webpack --colors --display-error-details --config=webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
